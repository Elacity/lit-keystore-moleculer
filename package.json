{
  "name": "@elacity-js/lit-keystore-moleculer",
  "version": "0.2.1",
  "description": "Keystore service on top of Lit protocol (decentralized key management)",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "node --loader ts-node/esm ./node_modules/moleculer/bin/moleculer-runner.mjs --config moleculer.config.ts services/**/*.service.ts",
    "dev:repl": "node --loader ts-node/esm ./node_modules/moleculer/bin/moleculer-runner.mjs --config moleculer.config.ts services/**/*.service.ts --hot --repl",
    "start": "moleculer-runner --config dist/moleculer.config.js",
    "test:types": "concurrently npm:prettier npm:lint npm:typecheck",
    "typecheck": "tsc --noEmit && echo \"tsc: no typecheck errors\"",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "cross-env TIMING=1 eslint . --ext cjs,mjs,js,jsx,ts,tsx",
    "lint:fix": "cross-env TIMING=1 eslint . --ext cjs,mjs,js,jsx,ts,tsx --fix",
    "prettier": "prettier . --ignore-unknown --check",
    "prettier:fix": "prettier . --ignore-unknown --write"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "WAU I/O",
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/jest": "^29.2.3",
    "@types/node": "^20.15.68",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3"
  },
  "dependencies": {
    "@lit-protocol/auth-helpers": "^7.2.0",
    "@lit-protocol/constants": "^7.2.0",
    "@lit-protocol/encryption": "^7.3.0",
    "@lit-protocol/lit-auth-client": "^7.3.1",
    "@lit-protocol/lit-node-client": "^7.2.0",
    "@types/elliptic": "^6.4.18",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.7",
    "elliptic": "^6.6.1",
    "eth-crypto": "^2.8.0",
    "ethers": "^5.8.0",
    "moleculer": "^0.14.27",
    "moleculer-repl": "^0.7.3",
    "nats": "^2.7.1",
    "node-cron": "^4.2.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.8.1",
    "typescript": "^4.9.3",
    "uuid": "^11.1.0"
  },
  "engines": {
    "node": ">= 20.x.x"
  }
}
